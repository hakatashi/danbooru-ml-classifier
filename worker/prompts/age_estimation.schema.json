{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "AnimeCharacterAgeEstimation",
  "type": "object",
  "properties": {
    "characters_detected": {
      "type": "integer",
      "minimum": 0
    },
    "characters": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "minimum": 1
          },
          "estimated_age_range": {
            "type": "string"
          },
          "most_likely_age": {
            "type": [
              "integer",
              "null"
            ],
            "minimum": 0
          },
          "confidence": {
            "type": "number",
            "minimum": 0.0,
            "maximum": 1.0
          },
          "gender_guess": {
            "type": "string",
            "enum": [
              "male",
              "female",
              "unknown"
            ]
          },
          "notes": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "estimated_age_range",
          "most_likely_age",
          "confidence",
          "gender_guess",
          "notes"
        ]
      }
    }
  },
  "required": [
    "characters_detected",
    "characters"
  ]
}